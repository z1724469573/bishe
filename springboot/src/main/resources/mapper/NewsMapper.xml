<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.mapper.NewsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.springboot.entity.NewsEntity">
        <id column="news_id" property="newsId"/>
        <result column="news_title" property="newsTitle"/>
        <result column="news_content" property="newsContent"/>
        <result column="news_type" property="newsType"/>
        <result column="news_date" property="newsDate"/>
        <result column="news_cover" property="newsCover"/>
        <result column="news_tag" property="newsTag"/>
        <result column="news_read" property="newsRead"/>
        <result column="news_state" property="newsState"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        news_id
        , news_title, news_content, news_type, news_date,news_cover,news_tag,news_read, news_state
    </sql>

</mapper>
