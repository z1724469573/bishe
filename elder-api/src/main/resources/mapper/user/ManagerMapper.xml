<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.elderapi.user.mapper.ManagerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.elderapi.user.entity.ManagerEntity">
        <id column="manager_id" property="id" />
        <result column="manager_acc" property="acc" />
        <result column="manager_pwd" property="pwd" />
        <result column="manager_name" property="name" />
        <result column="user_sex" property="userSex" />
        <result column="manager_age" property="age" />
        <result column="manager_phone" property="phone" />
        <result column="manager_address" property="address" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        manager_id, manager_acc, manager_pwd, manager_name, user_sex, manager_age, manager_phone, manager_address, status
    </sql>

</mapper>
