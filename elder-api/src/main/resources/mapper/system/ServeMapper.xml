<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.elderapi.system.mapper.ServeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.elderapi.system.entity.ServeEntity">
        <id column="serve_id" property="id" />
        <result column="serve_name" property="name" />
        <result column="serve_cover" property="cover" />
        <result column="serve_content" property="content" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        serve_id, serve_name, serve_cover, serve_content, status
    </sql>

</mapper>
