<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.elderapi.system.mapper.ActiveMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.elderapi.system.entity.ActiveEntity">
        <id column="active_id" property="id" />
        <result column="active_name" property="name" />
        <result column="active_start" property="start" />
        <result column="active_end" property="end" />
        <result column="active_address" property="address" />
        <result column="active_phone" property="phone" />
        <result column="active_content" property="content" />
        <result column="active_cover" property="cover" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        active_id, active_name, active_start, active_end, active_address, active_phone, active_content, active_cover, status
    </sql>

</mapper>
